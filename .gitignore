__pycache__/
*.py[cod]
*$py.class
*.so
apps/backend/__pycache__/
apps/backend/**/__pycache__/

# Virtual environments
venv/
.venv/
ENV/
env/
apps/backend/venv/
apps/backend/.venv/

# Python testing
.pytest_cache/
.coverage
.coverage.*
htmlcov/
*.cover
.hypothesis/

# Database
*.db
*.sqlite
*.sqlite3
apps/backend/*.db

# Alembic
apps/backend/alembic/versions/*.pyc

