# ============================================================================
# ENVIRONMENT VARIABLES (NEVER COMMIT!)
# ============================================================================
.env
.env.local
.env*.local
.env.development.local
.env.test.local
.env.production.local
apps/frontend/.env.local
apps/backend/.env
apps/backend/.env.local
*.env
!.env.example
!.env.local.example

# ============================================================================
